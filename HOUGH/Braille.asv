%% Braille - Image 1

clear variables;
close all;
clc;

I = im2double(imread("./Braille/braille1.png"));

% Filtrage --> Prétraitement


S = strel('disk',8);   % Element structurant
Ifiltered = imtophat(Ifiltered,S);
% figure()
% imshow(Ifiltered,[])


% Détection de cercles
[centers, radius] = imfindcircles(Ifiltered,[2 6],'ObjectPolarity','bright');
figure()
imshow(I,[])
viscircles(centers, radius,'Color','b')
